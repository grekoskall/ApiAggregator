{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GitHub": {
    "AccessToken": "fakeaccesstoken",
    "ApiBaseUrl": "https://api.github.com",
    "UserAgent": "ApiAggregation"
  },
  "OpenWeatherMap": {
    "ApiKey": "fakeapikey",
    "ApiBaseUrl": "https://api.openweathermap.org/data/2.5"
  },
  "NewsAPI": {
    "ApiKey": "fakeapikey",
    "ApiBaseUrl": "https://newsapi.org/v2"
  },
  "Spotify": {
    "ClientId": "fakeclientid",
    "ClientSecret": "fakesecret",
    "RedirectUri": "https://localhost:7001/api/aggregation/spotify/callback"
  },
  "FreeDictionary": {
    "ApiBaseUrl": "https://api.dictionaryapi.dev/api/v2/entries/en"
  },
  "Countries": {
    "ApiBaseUrl": "https://restcountries.com/v3.1"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 10
      },
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 100
      }
    ]
  },
  "ApplicationInsights": {
    "ConnectionString": "your-connection-string"
  }
}